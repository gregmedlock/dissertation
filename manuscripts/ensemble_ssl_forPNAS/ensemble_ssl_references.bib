@ARTICLE{Plata2012-ys,
  title    = "Global probabilistic annotation of metabolic networks enables
              enzyme discovery",
  author   = "Plata, Germ{\'a}n and Fuhrer, Tobias and Hsiao, Tzu-Lin and
